{
  "schema": "lcod-registry@1",
  "registries": [
    { "id": "official", "type": "git", "url": "https://github.com/lcod-team/lcod-registry" }
  ],
  "namespaces": {
    "lcod": { "owners": ["github:lcod-team"], "requireSignature": false },
    "tooling": { "owners": ["github:lcod-team"], "requireSignature": false }
  },
  "packages": [
    {
      "id": "lcod://tooling/registry/catalog/generate",
      "registryId": "official",
      "versionsPath": "packages/tooling/registry/catalog/versions.json"
    },
    {
      "id": "lcod://tooling/registry/source/load",
      "registryId": "official",
      "versionsPath": "packages/tooling/registry/source/versions.json"
    },
    {
      "id": "lcod://tooling/registry/resolution",
      "registryId": "official",
      "versionsPath": "packages/tooling/registry/resolution/versions.json"
    },
    {
      "id": "lcod://tooling/registry/index",
      "registryId": "official",
      "versionsPath": "packages/tooling/registry/index/versions.json"
    },
    {
      "id": "lcod://tooling/registry/select",
      "registryId": "official",
      "versionsPath": "packages/tooling/registry/select/versions.json"
    },
    {
      "id": "lcod://tooling/registry/fetch",
      "registryId": "official",
      "versionsPath": "packages/tooling/registry/fetch/versions.json"
    }
  ]
}
